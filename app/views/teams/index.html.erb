    <div class="row">
        <div class="col-md-8 offset-2">
                    <table class="table">
				<thead>
					<tr>
						<th></th>
						<th>チーム名</th>
						<th>都道府県</th>
                        <th>活動頻度</th>
                        <th>活動曜日</th>
					</tr>
				</thead>


				<tbody>
					<% @teams.each do |user| %>
					<tr>
						<td><%= link_to user_path(user), class: "user_#{user.id}"  do %>
                        <%= attachment_image_tag(user,:profile_image, fallback: "/assets/no-image.png", size:"60x60") %></td>
						<% end %>
                        <td><%= user.name%></td>
						<td><%= user.prefecture %></td>
						<td><%= user.gender_status_i18n %></td>
                        <td><%= user.age %>歳</td>
					</tr>
					<% end %>
				</tbody>
			</table>

        </div>
    </div>